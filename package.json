    "@rollup/plugin-replace": "3.0.0",
    "@rollup/plugin-typescript": "8.3.0",
    "@types/node": "16.11.2",
    "fs-extra": "8.1.0",
    "localforage": "1.10.0",
    "rollup": "2.58.0",
    "rollup-plugin-terser": "7.0.2",
    "tslib": "2.3.1",
    "typedoc": "0.22.6",
    "typedoc-plugin-missing-exports": "0.22.3",
    "typescript": "4.3.5"
